<OperatingSystem xmlns="http://resources.isystem.com/rtos_description" xmlns:it="http://www.w3.org/2001/XMLSchema-instance" it:schemaLocation="http://resources.isystem.com/rtos_description https://www.isystem.com/downloads/schemas/RTOS_description_0.xsd">
  <Name>STM32</Name>
  <NumCores>1</NumCores>
  <ORTI></ORTI>
  <Types>
    <TypeEnum>
      <Name>TypeEnum_ThreadMapping</Name>
      <Enum><Name>IDLE</Name><Value>0</Value></Enum>
      <Enum><Name>Tmr Svc</Name><Value>1</Value></Enum>
      <Enum><Name>LEDThread</Name><Value>2</Value></Enum>
      <Enum><Name>MessageQueuePro</Name><Value>3</Value></Enum>
      <Enum><Name>MessageQueueCon</Name><Value>4</Value></Enum>
    </TypeEnum>
    <TypeEnum>
      <Name>TypeEnum_TaskStateMapping</Name>
      <Enum><Name>RUNNING</Name><Value>1</Value></Enum>
      <Enum><Name>READY</Name><Value>2</Value></Enum>
      <Enum><Name>CREATED</Name><Value>3</Value></Enum>
      <Enum><Name>DELETED</Name><Value>4</Value></Enum>
      <Enum><Name>BLOCKED</Name><Value>5</Value></Enum>
      <Enum><Name>SUSPENDED</Name><Value>6</Value></Enum>
    </TypeEnum>
  </Types>
  <Profiler>
    <Object>
      <Name>Threads_Name</Name>
      <Definition>Threads_Definition</Definition>
      <Description>All Cores: Threads</Description>
      <Type>TypeEnum_ThreadMapping</Type>
      <DefaultValue>NO_THREAD</DefaultValue>
      <Expression>isystem_trace</Expression>
      <Level>Task</Level>
      <TaskState>
        <MaskID>0x0000FFFF</MaskID>
        <MaskState>0x00FF0000</MaskState>
        <Type>TypeEnum_TaskStateMapping</Type>
        <StateInfo>
          <Name>RUNNING</Name>
          <Property>Run</Property>
        </StateInfo>
        <StateInfo>
          <Name>READY</Name>
          <Property>Terminate</Property>
        </StateInfo>
        <StateInfo>
          <Name>DELETED</Name>
          <Property>Terminate</Property>
        </StateInfo>
        <StateInfo>
          <Name>BLOCKED</Name>
          <Property>Terminate</Property>
        </StateInfo>
        <StateInfo>
          <Name>SUSPENDED</Name>
          <Property>Terminate</Property>
        </StateInfo>
      </TaskState>
    </Object>
  </Profiler>
</OperatingSystem>